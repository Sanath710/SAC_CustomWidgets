{
    "id": "Custom_Export",
    "version": "1.0.0",
    "name": "Export Table to Excel",
    "description": "Multi-Header Table Export to Excel using Amcharts 4 library",
    "newInstancePrefix": "Export",
    "icon": "https://sanath710.github.io/sac_custom_widget/Export%20Table%20to%20Excel/exportTableIcon.png",
    "vendor": "Sanath Ezhuthachan",
    "eula": "",
    "license": "",
    "supportsMobile": true,
    "webcomponents": [
        {
            "kind": "main",
            "tag": "sample-export",
            "url": "http://127.0.0.1:5500/Export%20Data%20-%20AmCharts4/exportTable.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "resultset": {
            "type": "any"
        },
        "measures": {
            "type": "any"
        },
        "dimensions": {
            "type": "any"
        },
        "filters": {
            "type": "any"
        },
        "properties": {
            "type": "any"
        },
        "method": {
            "type": "string"
        }
    },
    "methods": {
        "exportResultSetToExcel": {
            "description": "transform (format) ResultSet",
            "parameters": [
                {
                    "name": "ResultSet",
                    "type": "any",
                    "description": "Resultset of DataSource"
                },
                {
                    "name": "Dimensions",
                    "type": "any",
                    "description": "[Object]"
                },
                {
                    "name": "Measures",
                    "type": "any",
                    "description": "[Object]"
                },
                {
                    "name": "Filters",
                    "type": "any",
                    "description": "[Array]"
                },
                {
                    "name": "Properties",
                    "type": "any",
                    "description": "[Array]"
                }
            ],
            "body":"this.resultset = ResultSet; this.dimensions = Dimensions; this.measures = Measures; this.filters = Filters; this.properties=Properties; this.method = 'exportToExcel';"
        }
    },
    "events": {
       
    }
}
